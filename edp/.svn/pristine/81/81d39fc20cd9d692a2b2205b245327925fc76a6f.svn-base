package com.cv.bis.security.exception;

/**
 * 
 * @author Subramanian Karuppiah
 *
 */
public class CVApplicationException extends Exception {

	public static final int SYSTEM_INIT_ERROR = 1;
	public static final int PROPERTIES_LOAD_ERROR = 2;
	public static final int SECURITY_UTIL_ERROR = 5;
	public static final int SECURITY_ALGORITHM_ERROR = 6;
	public static final int SECURITY_KEY_ACCESS_ERROR = 66;
	public static final int SECURITY_KEY_DRIVER_ERROR = 666;
	public static final int SECURITY_KEY_DB_GET_ERROR = 6666;
	
	public static final int UNKNOWN_USER = -1;
	public static final int UNKNOWN_ERROR = -9;

	/**
	 * CVApplicationException constructor
	 */
	public CVApplicationException() {
		super();
	}

	/**
	 * 
	 * @param message
	 */
	public CVApplicationException(String message) {
		super(message);
	}

	/**
	 * 
	 * @param exception
	 */
	public CVApplicationException(Exception exception) {
		super(exception);
	}

	/**
	 * 
	 * @param message
	 * @param exception
	 */
	public CVApplicationException(String message, Exception exception) {
		super(message, exception);
	}

}
