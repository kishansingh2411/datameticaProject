<assembly>
  <id>packager</id>
  <formats>
    <format>zip</format>
  </formats>
  <includeBaseDirectory>false</includeBaseDirectory>
  
  <fileSets>
    <fileSet>
      <directory>${project.parent.basedir}</directory>
      <outputDirectory>edp</outputDirectory>
      <useDefaultExcludes>true</useDefaultExcludes>
	    <excludes>
      	<exclude>**/.classpath</exclude>
        <exclude>**/.log</exclude>
        <exclude>**/.settings/**</exclude>
        <exclude>**/.project</exclude>
        <exclude>**/${project.build.directory}/**</exclude>
		<exclude>**/packager/**</exclude>
		<exclude>**/extensions/**</exclude>	
		<exclude>**/subject-areas/omniture/gold_vow_*_tbl/**</exclude>
		<exclude>**/subject-areas/omniture/gold_opt_hit_data_esp_tbl/**</exclude>
		<exclude>**/subject-areas/omniture/gold_stock36_hit_data_tbl/**</exclude>
		<exclude>**/subject-areas/omniture/incoming_hit_data_stock36_tbl/**</exclude>
		<exclude>**/subject-areas/omniture/incoming_hit_data_stock36_raw_tbl/**</exclude>
		<exclude>**/subject-areas/omniture/smith_kom_*_tbl/**</exclude>
		<exclude>**/subject-areas/omniture/smith_vow_*_tbl/**</exclude>
		<exclude>**/subject-areas/omniture/smith_*/**</exclude>
		<exclude>**/subject-areas/omniture/gold_vow_*_tbl/**</exclude>
		<exclude>**/subject-areas/omniture/gold_unified_*_tbl/**</exclude>
		<exclude>**/subject-areas/omniture/gold_aggregate_*/**</exclude>
		<exclude>**/subject-areas/omniture/outgoing_*_tbl/**</exclude>
		<exclude>**/subject-areas/omniture/bin/deployment_channel900.sh/**</exclude>
		<exclude>**/subject-areas/omniture/bin/deployment.sh/**</exclude>
		<exclude>**/subject-areas/omniture/bin/executor.sh/**</exclude>
		<exclude>**/subject-areas/omniture/bin/executor_channel900.sh/**</exclude>
		<exclude>**/subject-areas/omniture/bin/purge_*.sh/**</exclude>
		<exclude>**/subject-areas/omniture/bin/update_unified_*.sh/**</exclude>
		<exclude>**/subject-areas/omniture/bin/unified_*.sh/**</exclude>
		<exclude>**/subject-areas/omniture/bin/calculate_aggregate_*.sh/**</exclude>
		<exclude>**/subject-areas/ods/**</exclude>		
		<exclude>**/subject-areas/hulu/**</exclude>
		<exclude>**/subject-areas/bering_media/**</exclude>
		<exclude>**/subject-areas/ddp/**</exclude>
		<exclude>**/subject-areas/mrdvr/**</exclude>
		<exclude>**/subject-areas/channel_tuning/**</exclude>
		<exclude>**/subject-areas/hive/**</exclude>
		<exclude>**/utils/**</exclude>
		
		</excludes>
      <lineEnding>unix</lineEnding>
    </fileSet>
  </fileSets>
  
  <files>
  	<file>
  		<source>../subject-areas/omniture/extensions/pig-udf-bank/target/pig-udf-bank-${version}.jar</source>
  		<outputDirectory>edp/subject-areas/omniture//extensions/pig-udf-bank/lib</outputDirectory>
  	</file>
  	<file>
		<source>../modules/target/utils-${version}.jar</source>
		<outputDirectory>edp/modules/lib</outputDirectory>
  	</file>
  </files>
  
</assembly>
