package com.cablevision.shoppingcart;

import java.util.List;
import java.util.Map;

public class ShoppingCartResult {
	
	private List<CartProductListInfo> cartProductListInfo;
	
	private Map<String, MissingProductOffering> missingProductOfferingInfo;
	
	private long totalNoOfRequests;
	
	private long noOfSuccessfulResponses;
	
	private long noOfUnsucessfulResponses;

	public List<CartProductListInfo> getCartProductListInfo() {
		return cartProductListInfo;
	}

	public void setCartProductListInfo(List<CartProductListInfo> cartProductListInfo) {
		this.cartProductListInfo = cartProductListInfo;
	}

	public long getTotalNoOfRequests() {
		return totalNoOfRequests;
	}

	public void setTotalNoOfRequests(long totalNoOfRequests) {
		this.totalNoOfRequests = totalNoOfRequests;
	}

	public long getNoOfSuccessfulResponses() {
		return noOfSuccessfulResponses;
	}

	public void setNoOfSuccessfulResponses(long noOfSuccessfulResponses) {
		this.noOfSuccessfulResponses = noOfSuccessfulResponses;
	}

	public long getNoOfUnsucessfulResponses() {
		return noOfUnsucessfulResponses;
	}

	public void setNoOfUnsucessfulResponses(long noOfUnsucessfulResponses) {
		this.noOfUnsucessfulResponses = noOfUnsucessfulResponses;
	}

	public Map<String, MissingProductOffering> getMissingProductOfferingInfo() {
		return missingProductOfferingInfo;
	}

	public void setMissingProductOfferingInfo(Map<String, MissingProductOffering> missingProductOfferingInfo) {
		this.missingProductOfferingInfo = missingProductOfferingInfo;
	}

}
