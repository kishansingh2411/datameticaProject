set hive.exec.dynamic.partition=true;
set hive.exec.dynamic.partition.mode=nonstrict;

INSERT OVERWRITE TABLE ${hivevar:hive_database_name_gold}.${hivevar:gold_rsdvr_requests_last_rec_tbl} 

SELECT 
	case when GOLD_RSDVR_REQUESTS_LAST_REC.SCHEDULE_ID is null then WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING else GOLD_RSDVR_REQUESTS_LAST_REC.SCHEDULE_ID end as SCHEDULE_ID,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.HOME_ID else WORK_RSDVR_REQUESTS_DEDUP.HOME_ID_INCOMING end ) else GOLD_RSDVR_REQUESTS_LAST_REC.HOME_ID end as HOME_ID,
