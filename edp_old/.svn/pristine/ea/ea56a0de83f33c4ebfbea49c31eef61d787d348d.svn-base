	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.RECORDING_TYPE else WORK_RSDVR_REQUESTS_DEDUP.RECORDING_TYPE_INCOMING end ) else GOLD_RSDVR_REQUESTS_LAST_REC.RECORDING_TYPE end as RECORDING_TYPE,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.SAVE_ALL else WORK_RSDVR_REQUESTS_DEDUP.SAVE_ALL_INCOMING end ) else GOLD_RSDVR_REQUESTS_LAST_REC.SAVE_ALL end as SAVE_ALL,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.SAVE_LATEST else WORK_RSDVR_REQUESTS_DEDUP.SAVE_LATEST_INCOMING end ) else GOLD_RSDVR_REQUESTS_LAST_REC.SAVE_LATEST end as SAVE_LATEST,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.CREATE_TIME else WORK_RSDVR_REQUESTS_DEDUP.CREATE_TIME_INCOMING end ) else GOLD_RSDVR_REQUESTS_LAST_REC.CREATE_TIME end as CREATE_TIME,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.MODIFIED_TIME else WORK_RSDVR_REQUESTS_DEDUP.MODIFIED_TIME_INCOMING end ) else GOLD_RSDVR_REQUESTS_LAST_REC.MODIFIED_TIME end as MODIFIED_TIME,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.END_TIME_ADJUSTMENT else WORK_RSDVR_REQUESTS_DEDUP.END_TIME_ADJUSTMENT_INCOMING end ) else GOLD_RSDVR_REQUESTS_LAST_REC.END_TIME_ADJUSTMENT end as END_TIME_ADJUSTMENT,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.SAVE_DAYS else WORK_RSDVR_REQUESTS_DEDUP.SAVE_DAYS_INCOMING end ) else GOLD_RSDVR_REQUESTS_LAST_REC.SAVE_DAYS end as SAVE_DAYS,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.STATUS else WORK_RSDVR_REQUESTS_DEDUP.STATUS_INCOMING end ) else GOLD_RSDVR_REQUESTS_LAST_REC.STATUS end as STATUS,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.SHORT_TITLE else WORK_RSDVR_REQUESTS_DEDUP.SHORT_TITLE_INCOMING end ) else GOLD_RSDVR_REQUESTS_LAST_REC.SHORT_TITLE end as SHORT_TITLE,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.DS_STARTTIME else WORK_RSDVR_REQUESTS_DEDUP.DS_STARTTIME_INCOMING end ) else GOLD_RSDVR_REQUESTS_LAST_REC.DS_STARTTIME end as DS_STARTTIME,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.LAST_UPDATED else WORK_RSDVR_REQUESTS_DEDUP.LAST_UPDATED_INCOMING end ) else GOLD_RSDVR_REQUESTS_LAST_REC.LAST_UPDATED end as LAST_UPDATED,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.MAC_ADDRESS_IND else WORK_RSDVR_REQUESTS_DEDUP.MAC_ADDRESS_IND end ) else GOLD_RSDVR_REQUESTS_LAST_REC.MAC_ADDRESS_IND end          AS MAC_ADDRESS_IND,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.CALL_SIGN_IND else WORK_RSDVR_REQUESTS_DEDUP.CALL_SIGN_IND end ) else GOLD_RSDVR_REQUESTS_LAST_REC.CALL_SIGN_IND end            AS CALL_SIGN_IND,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.START_DATE_IND else WORK_RSDVR_REQUESTS_DEDUP.START_DATE_IND end ) else GOLD_RSDVR_REQUESTS_LAST_REC.START_DATE_IND end          AS START_DATE_IND,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.START_TIME_IND else WORK_RSDVR_REQUESTS_DEDUP.START_TIME_IND end ) else GOLD_RSDVR_REQUESTS_LAST_REC.START_TIME_IND end      AS START_TIME_IND,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.DURATION_IND else WORK_RSDVR_REQUESTS_DEDUP.DURATION_IND end ) else GOLD_RSDVR_REQUESTS_LAST_REC.DURATION_IND end           AS DURATION_IND,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.RECORDING_TYPE_IND else WORK_RSDVR_REQUESTS_DEDUP.RECORDING_TYPE_IND end ) else GOLD_RSDVR_REQUESTS_LAST_REC.RECORDING_TYPE_IND end      AS RECORDING_TYPE_IND,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.SAVE_ALL_IND else WORK_RSDVR_REQUESTS_DEDUP.SAVE_ALL_IND end ) else GOLD_RSDVR_REQUESTS_LAST_REC.SAVE_ALL_IND end         AS SAVE_ALL_IND,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.SAVE_LATEST_IND else WORK_RSDVR_REQUESTS_DEDUP.SAVE_LATEST_IND end ) else GOLD_RSDVR_REQUESTS_LAST_REC.SAVE_LATEST_IND end         AS SAVE_LATEST_IND,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.MODIFIED_TIME_IND else WORK_RSDVR_REQUESTS_DEDUP.MODIFIED_TIME_IND end ) else GOLD_RSDVR_REQUESTS_LAST_REC.MODIFIED_TIME_IND end       AS MODIFIED_TIME_IND,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.END_TIME_ADJUSTMENT_IND else WORK_RSDVR_REQUESTS_DEDUP.END_TIME_ADJUSTMENT_IND end ) else GOLD_RSDVR_REQUESTS_LAST_REC.END_TIME_ADJUSTMENT_IND end AS END_TIME_ADJUSTMENT_IND,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.SAVE_DAYS_IND else WORK_RSDVR_REQUESTS_DEDUP.SAVE_DAYS_IND end ) else GOLD_RSDVR_REQUESTS_LAST_REC.SAVE_DAYS_IND end AS SAVE_DAYS_IND,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.STATUS_IND else WORK_RSDVR_REQUESTS_DEDUP.STATUS_IND end ) else GOLD_RSDVR_REQUESTS_LAST_REC.STATUS_IND end  AS STATUS_IND,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.SHORT_TITLE_IND else WORK_RSDVR_REQUESTS_DEDUP.SHORT_TITLE_IND end ) else GOLD_RSDVR_REQUESTS_LAST_REC.SHORT_TITLE_IND end  AS SHORT_TITLE_IND,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.DS_STARTTIME_IND else WORK_RSDVR_REQUESTS_DEDUP.DS_STARTTIME_IND end ) else GOLD_RSDVR_REQUESTS_LAST_REC.DS_STARTTIME_IND end  AS DS_STARTTIME_IND,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.LAST_UPDATED_IND else WORK_RSDVR_REQUESTS_DEDUP.LAST_UPDATED_IND end ) else GOLD_RSDVR_REQUESTS_LAST_REC.LAST_UPDATED_IND end AS LAST_UPDATED_IND,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.LOAD_DATE else WORK_RSDVR_REQUESTS_DEDUP.LOAD_DATE end ) else GOLD_RSDVR_REQUESTS_LAST_REC.LOAD_DATE end as LOAD_DATE,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG='TRUE' and WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG='FALSE' then WORK_RSDVR_REQUESTS_DEDUP.OP_TYPE_U else WORK_RSDVR_REQUESTS_DEDUP.OP_TYPE_I end ) else GOLD_RSDVR_REQUESTS_LAST_REC.OP_TYPE end as OP_TYPE,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL  then GOLD_RSDVR_REQUESTS_LAST_REC.DTM_CREATED else (case when WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG='TRUE' then  WORK_RSDVR_REQUESTS_DEDUP.DTM_SYSDATE else GOLD_RSDVR_REQUESTS_LAST_REC.DTM_CREATED end) end )  else GOLD_RSDVR_REQUESTS_LAST_REC.DTM_CREATED end  as DTM_CREATED,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.RSDVR_REQUESTS_DELTA_SEQ else WORK_RSDVR_REQUESTS_DEDUP.RSDVR_REQUESTS_DELTA_SEQ end ) else GOLD_RSDVR_REQUESTS_LAST_REC.RSDVR_REQUESTS_DELTA_SEQ end AS RSDVR_REQUESTS_DELTA_SEQ,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then WORK_RSDVR_REQUESTS_DEDUP.DTM_LAST_MODIFIED else WORK_RSDVR_REQUESTS_DEDUP.DTM_SYSDATE end ) else GOLD_RSDVR_REQUESTS_LAST_REC.DTM_LAST_MODIFIED end  as DTM_LAST_MODIFIED,
	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null  then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.DTM_LAST_UPDATED else (case when WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG !='TRUE' then  WORK_RSDVR_REQUESTS_DEDUP.DTM_SYSDATE end) end ) else GOLD_RSDVR_REQUESTS_LAST_REC.DTM_LAST_UPDATED end  as DTM_LAST_UPDATED
FROM 
	${hivevar:hive_database_name_gold}.${hivevar:gold_rsdvr_requests_last_rec_tbl} as GOLD_RSDVR_REQUESTS_LAST_REC FULL OUTER JOIN ${hivevar:hive_database_name_work}.${hivevar:work_rsdvr_requests_dedup_tbl} as WORK_RSDVR_REQUESTS_DEDUP 
ON
	(GOLD_RSDVR_REQUESTS_LAST_REC.SCHEDULE_ID = WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING AND
	 GOLD_RSDVR_REQUESTS_LAST_REC.HOME_ID = WORK_RSDVR_REQUESTS_DEDUP.HOME_ID_INCOMING AND
	GOLD_RSDVR_REQUESTS_LAST_REC.SERIAL_NUMBER = WORK_RSDVR_REQUESTS_DEDUP.SERIAL_NUMBER_INCOMING)
;

