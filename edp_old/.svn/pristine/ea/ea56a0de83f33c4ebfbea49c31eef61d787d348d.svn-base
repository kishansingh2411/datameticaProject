	case when WORK_RSDVR_REQUESTS_DEDUP.SCHEDULE_ID_INCOMING is not null then ( case when WORK_RSDVR_REQUESTS_DEDUP.CHANGEDFLAG IS NULL AND WORK_RSDVR_REQUESTS_DEDUP.NEWFLAG IS NULL then GOLD_RSDVR_REQUESTS_LAST_REC.MAC_ADDRESS else WORK_RSDVR_REQUESTS_DEDUP.MAC_ADDRESS_INCOMING end ) else GOLD_RSDVR_REQUESTS_LAST_REC.MAC_ADDRESS end as MAC_ADDRESS,
