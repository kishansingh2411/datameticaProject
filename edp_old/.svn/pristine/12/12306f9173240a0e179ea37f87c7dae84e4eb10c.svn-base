# Defines project name that is used in different path context. It shall not     
# contain any special characters. It shall only contain alphanumeric character     
# or underscores or hypens. It shall not start with number             
SUBJECT_AREA_NAME=ovcdr

LOCAL_DATA_DIRECTORY=/UTIL/inbound/incoming/ovcdr

#Define directory path to be used for different layers of directory                                                               
DATA_LAYER_DIR_INCOMING_SUBJECT_AREA=${DATA_LAYER_DIR_INCOMING}/${SUBJECT_AREA_NAME}

DATA_LAYER_DIR_GOLD_SUBJECT_AREA=${DATA_LAYER_DIR_GOLD}/${SUBJECT_AREA_NAME}

DATA_LAYER_DIR_ARCHIVE_SUBJECT_AREA=${DATA_LAYER_DIR_ARCHIVE}/${SUBJECT_AREA_NAME}

# Defines log directory name that is used in different path context.            
LOG_DIR_SUBJECT_AREA=${LOG_DIR_PREFIX}/app/edp/log/$SUBJECT_AREA_NAME


## Define processing and landing directory for omniture daatset
DATA_LAYER_DIR_INCOMING_SUBJECT_AREA_LANDING=${DATA_LAYER_DIR_INCOMING}/${SUBJECT_AREA_NAME}/landing

DATA_LAYER_DIR_INCOMING_SUBJECT_AREA_PROCESSING=${DATA_LAYER_DIR_INCOMING}/${SUBJECT_AREA_NAME}/processing

DATA_LAYER_DIR_INCOMING_SUBJECT_AREA_DATA=${DATA_LAYER_DIR_INCOMING}/${SUBJECT_AREA_NAME}/data

# Defines log directory name that is used in different path context.            
BATCH_ID_DIR_SUBJECT_AREA=${LOG_DIR_PREFIX}/app/edp/work/batch

