INSERT OVERWRITE TABLE ${hivevar:hive_database_name_work}.${hivevar:work_rsdvr_schedule_dedup_tbl}
SELECT 
	callsign_incoming             ,
	startdate_incoming            ,
	starttime_incoming            ,
	duration_incoming             ,
	short_title_incoming          ,
	long_title_incoming           ,
	theme_ids_incoming            ,
	short_desc_incoming           ,
	long_desc_incoming            ,
	mpaa_rating_incoming          ,
	tv_rating_incoming            ,
	stereo_incoming               ,
	surround_incoming             ,
	sap_incoming                  ,
	closedcaption_incoming        ,
	animated_incoming             ,
	blackwhite_incoming           ,
	rerun_incoming                ,
	live_incoming                 ,
	ismovie_incoming              ,
	nudity_incoming               ,
	adultlang_incoming            ,
	violence_incoming             ,
	adulttheme_incoming           ,
	halfstars_incoming            ,
	hdtv_incoming                 ,
	cvc_callsign_incoming         ,
	loadtime                      ,
	startdate_starttime_incoming   ,
    ds_starttime_incoming          ,
    last_updated_incoming          ,  
	callsign_gold                 ,
	startdate_gold                ,
	starttime_gold                ,
	duration_gold                 ,
	shorttitle_gold               ,
	longtitle_gold                ,
	themeids_gold                 ,
	shortdesc_gold                ,
	longdesc_gold                 ,
	mpaarating_gold               ,
	tvrating_gold                 ,
	stereo_gold                   ,
	surround_gold                 ,
	sap_gold                      ,
	closedcaption_gold            ,
	animated_gold                 ,
	blackwhite_gold               ,
	rerun_gold                    ,
	live_gold                     ,
	ismovie_gold                  ,
	nudity_gold                   ,
	adultlang_gold                ,
	violence_gold                 ,
	adulttheme_gold               ,
	halfstars_gold                ,
	hdtv_gold                     ,
	cvccallsign_gold              ,
	startdate_starttime_gold      ,
    ds_starttime_gold             ,
    last_updated_gold             ,
	op_type                       ,
	shorttitle_ind                ,
	longtitle_ind                 ,
	themeids_ind                  ,
	shortdesc_ind                 ,
	longdesc_ind                  ,
	mpaarating_ind                ,
	tvrating_ind                  ,
	stereo_ind                    ,
	surround_ind                  ,
	sap_ind                       ,
	closedcaption_ind             ,
	animated_ind                  ,
	blackwhite_ind                ,
	rerun_ind                     ,
	live_ind                      ,
	ismovie_ind                   ,
	nudity_ind                    ,
	adultlang_ind                 ,
	violence_ind                  ,
	adulttheme_ind                ,
	halfstars_ind                 ,
	hdtv_ind                      ,
	cvccallsign_ind               ,
	changedflag                   ,
	newflag                       ,
	load_date                     ,
	dtm_sysdate                   ,
	y_ind                         ,
	op_type_i                     ,
	op_type_u                     ,
	rsdvr_schedule_delta_seq      	
FROM 
(SELECT seq.*,ROW_NUMBER () OVER (PARTITION BY callsign_incoming,startdate_incoming,starttime_incoming,duration_incoming ORDER BY loadtime DESC ) AS RECORD_ROW_NUM
FROM 
	${hivevar:hive_database_name_work}.${hivevar:work_rsdvr_schedule_seq_tbl} seq
WHERE 
	CHANGEDFLAG = 'TRUE' OR NEWFLAG='TRUE'
)dedup
WHERE
	dedup.RECORD_ROW_NUM = 1
;

