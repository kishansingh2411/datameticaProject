set hive.exec.dynamic.partition=true;
set hive.exec.dynamic.partition.mode=nonstrict;

INSERT OVERWRITE TABLE ${hivevar:hive_database_name_gold}.${hivevar:gold_rsdvr_requests_archive_last_rec} 
select 
GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.SCHEDULE_ID             
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.HOME_ID                 
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.SERIAL_NUMBER           
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.MAC_ADDRESS             
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.CALL_SIGN               
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.START_DATE              
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.START_TIME              
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.DURATION                
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.RECORDING_TYPE          
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.SAVE_ALL                
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.SAVE_LATEST             
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.CREATE_TIME             
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.MODIFIED_TIME           
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.END_TIME_ADJUSTMENT     
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.SAVE_DAYS               
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.STATUS                  
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.SHORT_TITLE             
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.ARCHIVE_DATE_EST            
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.SOURCE_LOAD_DATE            
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.MAC_ADDRESS_IND         
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.CALL_SIGN_IND           
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.START_DATE_IND          
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.START_TIME_IND          
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.DURATION_IND            
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.RECORDING_TYPE_IND      
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.SAVE_ALL_IND            
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.SAVE_LATEST_IND         
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.MODIFIED_TIME_IND       
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.END_TIME_ADJUSTMENT_IND 
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.SAVE_DAYS_IND           
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.STATUS_IND              
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.SHORT_TITLE_IND         
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.ARCHIVE_DATE_EST_IND        
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.SOURCE_LOAD_DATE_IND        
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.CREATE_TIME_IND
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.LOAD_DATE               
,case when WORK_RSDVR_REQUESTS_DELETE_DEDUP.SCHEDULE_ID IS NOT NULL AND WORK_RSDVR_REQUESTS_DELETE_DEDUP.HOME_ID IS NOT NULL AND WORK_RSDVR_REQUESTS_DELETE_DEDUP.SERIAL_NUMBER IS NOT NULL then WORK_RSDVR_REQUESTS_DELETE_DEDUP.OP_TYPE else GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.OP_TYPE end as OP_TYPE 
,GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.DTM_CREATED  as DTM_CREATED  
,case when WORK_RSDVR_REQUESTS_DELETE_DEDUP.SCHEDULE_ID IS NOT NULL AND WORK_RSDVR_REQUESTS_DELETE_DEDUP.HOME_ID IS NOT NULL AND WORK_RSDVR_REQUESTS_DELETE_DEDUP.SERIAL_NUMBER IS NOT NULL then WORK_RSDVR_REQUESTS_DELETE_DEDUP.RSDVR_REQUESTS_ARCHIVE_DELTA_SEQ else GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.RSDVR_REQUESTS_ARCHIVE_DELTA_SEQ end as RSDVR_REQUESTS_ARCHIVE_DELTA_SEQ         
,case when WORK_RSDVR_REQUESTS_DELETE_DEDUP.SCHEDULE_ID IS NOT NULL AND WORK_RSDVR_REQUESTS_DELETE_DEDUP.HOME_ID IS NOT NULL AND WORK_RSDVR_REQUESTS_DELETE_DEDUP.SERIAL_NUMBER IS NOT NULL  then current_timestamp  else GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.DTM_LAST_MODIFIED end  as DTM_LAST_MODIFIED
,case when WORK_RSDVR_REQUESTS_DELETE_DEDUP.SCHEDULE_ID IS NOT NULL AND WORK_RSDVR_REQUESTS_DELETE_DEDUP.HOME_ID IS NOT NULL AND WORK_RSDVR_REQUESTS_DELETE_DEDUP.SERIAL_NUMBER IS NOT NULL then  current_timestamp  else GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.DTM_SOURCE_LOAD_DATE end  as DTM_SOURCE_LOAD_DATE 
FROM 
 ${hivevar:hive_database_name_gold}.${hivevar:gold_rsdvr_requests_archive_last_rec} GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC FULL OUTER JOIN ${hivevar:hive_database_name_work}.${hivevar:work_rsdvr_requests_archive_delete_dedup} WORK_RSDVR_REQUESTS_ARCHIVE_DELETE_DEDUP   
ON
	(GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.SCHEDULE_ID = WORK_RSDVR_REQUESTS_DELETE_DEDUP.SCHEDULE_ID AND
	 GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.HOME_ID = WORK_RSDVR_REQUESTS_DELETE_DEDUP.HOME_ID AND
	GOLD_RSDVR_REQUESTS_ARCHIVE_LAST_REC.SERIAL_NUMBER = WORK_RSDVR_REQUESTS_DELETE_DEDUP.SERIAL_NUMBER)
;